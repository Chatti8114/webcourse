package java_0611;

class  ByteOverflow
{
	public static void main(String[] args) 
	{
   short bb = 32760;
   int    ii = 0;
   for(int x = 0; x <= 101; x++)
		{
	     System.out.print(bb++);
	     System.out.print("\t\t");
	     System.out.println(ii++);

		}

		System.out.println("300 + bb ="+(300+bb));
		System.out.println("Hello World!");
	}
}
