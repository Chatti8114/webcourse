package java_0619;

public class Array_1_1
{
	public static void main(String[] args) 
	{
		int[] month = new int[] {31,28,31,30,31,30,31,31,30,31,30,31};
        for (int i =0 ; i<month.length ; i++ )
		{
			System.out.println((i+1)+"�� : "+month[i]+"��");
		}
	
		System.out.println("Hello World !");
	}
}
